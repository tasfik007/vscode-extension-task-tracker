{
  "name": "todo-list",
  "displayName": "todo-list",
  "description": "A todo app to track user's progress",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
    "activitybar": [
      {
        "id": "my-todo-extension",
        "title": "Tasfik's todo",
        "icon": "images/todo-icon-2.png"
      }
    ]
  },
  "views": {
    "my-todo-extension": [
      {
        "id": "todoListView",
        "name": "Todo List"
      }
    ]
  },
    "commands": [{
      "command": "todo-list.todo",
      "title": "todo"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
